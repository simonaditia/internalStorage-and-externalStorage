<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aplikasiandroid" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplikasiAndroid" >
        <activity android:name=".ExternalStorageActivity" >
        </activity>
        <activity android:name=".InternalStorageActivity" />
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Menu1"
            android:label="Aplikasi Mobile" />
        <activity
            android:name=".Menu2"
            android:label="Aplikasi Kalkulator" />
        <activity
            android:name=".Menu3"
            android:label="Aplikasi List View" />
    </application>

</manifest>